#include<stdio.h>
#define LENGTH 5
int i=0,j=0;
int main()
{
    int x[LENGTH][LENGTH];
    int trace=0;
    for ( i = 0; i < LENGTH; i++)
   {
    for ( j = 0; j < LENGTH; j++)
    {
        printf("enter the A%d%d\n",i+1,j+1); 
        scanf("%d",&x[i][j]);
    }
   }
   
    for ( i = 0; i < LENGTH; i++)
   {
    for ( j = 0; j < LENGTH; j++)
    {
        printf("%d",x[i][j]); 
        
    }
    printf("\n");
   }
for ( i = 0; i < LENGTH; i++)
{

     trace+=x[i][i];
      
    
}
printf("%d",trace);
return 0;
}
